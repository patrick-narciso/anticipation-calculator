{
  "presets": [
    "@babel/env",
    [
      "@babel/preset-react",
      {
        "pragma": "dom",
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@": "./src/",
          "@Uikit": "./src/components/Uikit"
        }
      }
    ],
    "emotion"
  ]
}
